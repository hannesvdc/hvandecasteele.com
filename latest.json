---
layout: null
permalink: /blog/latest.json
---
[
{% assign posts = site.posts | slice: 0, 10 %}
{% for post in posts %}
  {
    "title": {{ post.title | jsonify }},
    "url": {{ post.url | absolute_url | jsonify }},
    "date": {{ post.date | date_to_xmlschema | jsonify }},
    "substack_url": {{ post.substack_url | default: "" | jsonify }}
  }{% unless forloop.last %},{% endunless %}
{% endfor %}
]